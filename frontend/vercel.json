{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "CI": "false"
    }
  },
  "env": {
    "REACT_APP_NOTIFY_URL": "https://sua-dukens-projects-866eede9.vercel.app",
    "REACT_APP_GRAPHQL_URL": "http://localhost:4001/graphql",
    "REACT_APP_USER_API_URL": "http://localhost:5001/api",
    "REACT_APP_BOOK_API_URL": "http://localhost:8000",
    "REACT_APP_REVIEW_API_URL": "http://localhost:5003",
    "REACT_APP_LOAN_API_URL": "http://localhost:5002/api",
    "REACT_APP_RECOMMENDATION_API_URL": "http://localhost:8001",
    "REACT_APP_STATISTICS_API_URL": "https://statistics-service-qjzt.onrender.com/api"
  }
}
